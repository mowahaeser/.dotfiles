#!/usr/bin/env zsh

export DOTFILES=$HOME/.dotfiles

PERSONAL=$HOME/.config/personal

# provides fuctionalities for environment 
# addToPath - add new environment variables to PATH
source $PERSONAL/env

# Aliases go here
source $PERSONAL/aliases

export GOPATH=$HOME/devenv/go

addToPath $HOME/.local/go/bin
addToPath $HOME/usr/local/bin
addToPath $HOME/.local/bin
